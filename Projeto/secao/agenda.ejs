			<div class="pagPadrao col-md-8">
				<h1>Calendário</h1>
				<div id='calendar'></div>

			</div>
			<!-- / Calendário-->

			<!-- Botões-->
			<div class="botoesCalendario col-md-2">

				<div class="btn-group btn-group-toggle" data-toggle="buttons">
					<button type="button" class="btn btn-outline-secondary" name="options" id="option2>Secondary"><img src="/imgs/plus.png" width="20" height="20"></button>
