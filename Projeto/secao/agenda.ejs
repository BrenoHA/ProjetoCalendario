		<div class="row justify-content-center">
			<div class="tabela col-md-10">
				
				<h1>Tabela</h1>
				

				<table class="table table-striped table-hover table-condensed">
	                <thead>
	                    <tr>
	                        <th>Aula dada</th>
	                        <th>Professor</th>
	                        <th>Matéria</th>
	                        <th>Tipo de aula</th>
	                        <th>Aluno</th>
	                        <th>Horário</th>
	                    </tr>
	                </thead>

	                <tbody>
	                    <tr>
	                    	<% agendados.forEach( agend => { %>
	                    	

	                        <td>
	                        	<div class="radio">
				                    <label>
				                        <input type="radio" id="radioAula" idname="AULA_DADA" value="true"> Sim
				                    </label>
				                    <label>
				                        <input type="radio" idname="AULA_DADA" value="false" selected> Não
				                    </label>
				                </div>
	                        </td>
	                        <td><% agend.PROFESSOR %></td>
	                        <td><% agend.MATÉRIA %></td>
	                        <td><% agend.TIPO_DE_AULA %></td>
	                        <td><% agend.ALUNO %></td>
	                        <td><% agend.HORÁRIO_DA_AULA %></td>
	                    </tr>
	                   <% }) %>
	                </tbody>
          		</table>


